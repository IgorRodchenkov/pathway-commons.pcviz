# Pathway commons
pathwaycommons.url=http://www.pathwaycommons.org/pc2/

# BioGene
biogene.url=http://cbio.mskcc.org/biogene/
biogene.format=json

# iHop
ihop.url=http://www.ihop-net.org/UniPub/iHOP/

# Cocitation manager
cocitation.shelflife=30
cocitation.cache.folder=/tmp/cocitations
cocitation.cache.folder.test=/tmp/cocitations-test

# Pre-calculated networks and screenshots
precalculated.folder=/tmp/precalculated

# If the number of cocitations is below is this number, don't show it
cocitation.min.edge=0
cocitation.min.node=0

# HGNC util
#hgnc.location=http://www.genenames.org/cgi-bin/hgnc_downloads?col=gd_hgnc_id&col=gd_app_sym&col=gd_aliases&col=md_prot_id&status=Approved&status_opt=2&where=&order_by=gd_hgnc_id&format=text&limit=&hgnc_dbtag=on&submit=submit
hgnc.location=file:/tmp/hgnc.txt
hgnc.test.location=classpath:hgnc_test.txt
autoCompleteResult.limit=10

# CBioPortal
cbioportal.cache.folder=/tmp/cocitations/portal
cbioportal.cache.folder.test=/tmp/cocitations/portal-test

# PCViz self URL
pcviz.url=http://awabi.cbio.mskcc.org/pc2-demo/
